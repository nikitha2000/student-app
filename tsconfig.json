{ 
    "compilerOptions": { 
        "target": "es5", 
        "module": "commonjs", 
        "jsx": "react", 
        "strict": true, 
        "esModuleInterop": true, 
        "skipLibCheck": true, 
        "forceConsistentCasingInFileNames": true, 
        "moduleResolution": "node", 
        "resolveJsonModule": true, 
        "isolatedModules": true, 
        "noEmit": true 
        }, 
        "include": [
            "src/**/*.ts",
            "src/**/*.tsx"
        ] 
    }